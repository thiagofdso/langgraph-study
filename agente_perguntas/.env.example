# Copie este arquivo para agente_perguntas/.env e preencha as variáveis abaixo.
# GEMINI_API_KEY é obrigatório para que o agente consiga chamar o modelo.
GEMINI_API_KEY=

# Opcional: override do modelo, temperatura e limite de confiança.
GEMINI_MODEL=gemini-2.5-flash
GEMINI_TEMPERATURE=0.2
AGENTE_PERGUNTAS_CONFIDENCE=0.7

# Diretório onde os logs estruturados serão gravados.
AGENTE_PERGUNTAS_LOG_DIR=agente_perguntas/logs
